<table class="table table-dark table-hover">
    <thead>
        <th scope="col">Nombre</th>
        <th scope="col">Id</th>
    </thead>
    <tbody>
@foreach (var obj in Model)
{
    <tr>
        <td>@obj.Nombre</td>
        <td>@obj.Id</td>
        <td>
              @Html.ActionLink("Editar", "Update", new { id = obj.Id })
        </td>
        <td>
              @Html.ActionLink("Eliminar", "Delete", new { id = obj.Id })
        </td>
    </tr>
}
    </tbody>
</table>