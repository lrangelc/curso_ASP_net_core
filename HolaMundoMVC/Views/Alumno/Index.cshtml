@model Alumno

@{
    ViewData["Title"] = "Informacion Alumno";
    Layout = "_Simple";
}

<h1>@ViewData["Title"]</h1>

<strong>Desde la vista normal</strong>
<p>
    Nombre: @Model.Nombre
    <br/>
    Id: @Model.Id
</p>
<p>
    Curso: @Model.CursoId
</p>
<p>
    Curso: @Model.Curso.Nombre
</p>

<strong>Desde la vista parcial</strong>
<partial name="_ObjetoEscuelaSimple"></partial>